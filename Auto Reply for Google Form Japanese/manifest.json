{
    "manifest_version": 3,
    "name": "Auto Reply Google Form for Japanese",
    "version": "0.1.0",
    "description": "Googleフォームに自動返信を設定する",
    "icons": {
        "16": "icon/16.png",
        "32": "icon/32.png",
        "48": "icon/48.png",
        "128": "icon/128.png"
    },
    "action": {},
    "author": "oshima@automate-growth.com",
    "background": {
        "service_worker": "background/background.js"
    },
    "content_scripts": [{
        "matches": ["https://docs.google.com/forms/d/*"],
        "js": ["contents/contents-script.js"]
    }],
    "permissions": [
        "identity",
        "identity.email",
        "storage"
    ],
    "key":"",
    "oauth2":{
        "client_id":"54176729865-4ru4q55brq1862sh78slpljqu6d2olv9.apps.googleusercontent.com",
        "scopes":[
            "https://www.googleapis.com/auth/gmail.modify",
            "https://www.googleapis.com/auth/forms.body.readonly",
            "https://www.googleapis.com/auth/script.projects",
            "https://www.googleapis.com/auth/script.scriptapp",
            "https://www.googleapis.com/auth/script.deployments",
            "https://www.googleapis.com/auth/drive"
        ]
    }
}